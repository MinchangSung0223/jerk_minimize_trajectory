function drawPlot(x_H,x_H2,H,J)
    q1_list = [];
    q2_list = [];
    v1_list = [];
    v2_list = [];
    a1_list = [];
    a2_list = [];
    j1_list = [];
    j2_list = [];
    
    for i = 1:1:H+1
        q1 = x_H(i*J+1:(i+1)*J);
        q2 = x_H2(i*J+1:(i+1)*J);
        q1_list=[q1_list;q1'];
        
    end
    
    
end